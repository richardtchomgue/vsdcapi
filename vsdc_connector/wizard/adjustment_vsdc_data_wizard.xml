<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="vsdc_adjustment_wizard_form" model="ir.ui.view">
            <field name="name">vsdc.adjustment.wizard.form</field>
            <field name="model">vsdc.adjustment.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="is_synced" readonly="1"/>
                                <field name="synced_date" readonly="1"/>
                            </group>
                        </group>
                        <group>
                            <field name="date_sent" readonly="1"/>
                            <field name="response" readonly="1"/>
                        </group>
                        <footer>
                            <button name="action_ok" string="Ok" type="object" default_focus="1" class="oe_highlight"/>
                        </footer>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>